{
  "api": {
    "gameslookupapi": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "gameslookupfunction"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "auth": {
    "GamesLookup": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS",
            "REQUIRES_UPPERCASE"
          ],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [],
        "socialProviders": [
          "GOOGLE"
        ],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "gameslookupfunction": {
      "build": true,
      "dependsOn": [
        {
          "attributeEnvMap": {
            "Arn": "TABLE_ARN",
            "Name": "TABLE_NAME"
          },
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "storage",
          "resourceName": "gameslookupdb"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "gameslookupdb": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}